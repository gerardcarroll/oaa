    <dimen name="communities_card_width">200dp</dimen>
    <dimen name="communities_card_height">190dp</dimen>
    <dimen name="communities_card_text_width">190dp</dimen>

    <!-- Gallery Columns -->
    <integer name="communities_columns">5</integer>
</resources>
